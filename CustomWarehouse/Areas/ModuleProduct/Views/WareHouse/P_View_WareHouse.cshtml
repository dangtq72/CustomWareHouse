@using CW_Info;
@{
    WareHouse_Info _WareHouse_Info = new WareHouse_Info();
    if (ViewBag.obj != null)
    {
        _WareHouse_Info = ViewBag.obj;
    }
}
<br />

<div class="" style="padding-left: 20px;">
    <div class="col-11" style="margin-left: 10px;padding: 0;">
        <table class=" clsTableForView ">
            <tbody>
                <tr>
                    <td>Mã Kho</td>
                    <td>@_WareHouse_Info.WareHouse_Code</td>
                </tr>

                <tr>
                    <td style="vertical-align:top">Tên kho</td>
                    <td style="white-space:normal">@_WareHouse_Info.WareHouse_Name</td>
                </tr>
                <tr>
                    <td>Vị trí</td>
                    <td>@_WareHouse_Info.Location</td>
                </tr>
            </tbody>
        </table>

    </div>
    <div class="form-group">
        <div class="col-11" style="border-top: 1px solid #7b68ee;margin-left:10px;margin-top: 10px;padding-top: 10px;padding-left: 0;padding-right: 0;">
            <input type="button" class="classBtn classBtn-Default" value="Thoát" onclick="CloseViewWareHouse()" style="width: auto;float: right" />
        </div>
    </div>
</div>