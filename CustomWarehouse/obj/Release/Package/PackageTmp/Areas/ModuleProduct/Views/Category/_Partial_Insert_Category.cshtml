@using CW_Info;
<style>
    .row-input {
        margin-top: 9px;
    }

    .ms-ul ul li {
        text-align: left;
    }

    .ms-ul input {
        width: 10%;
    }
</style>
<div class="form-row" style="padding:10px;padding-right:0;padding-left:20px">
    <div id="@("d_mess_InsertCategory")" class="d_Message"></div>

    <div class="row-input col-4-6">
        Mã sản phẩm <i style="color: red">(*)</i>
    </div>
    <div class="row-input col-6-4">
        <input type="text" id="@("_txtCategoryCode")" maxlength="100" onkeydown="PressEnterInsertCategory(event)" />
    </div>
    <<div class="row-input col-4-6">
        Tên sản phẩm <i style="color: red">(*)</i>
    </div>
    <div class="row-input col-6-4">
        <input type="text" id="@("_txtCategoryName")" maxlength="100" onkeydown="PressEnterInsertCategory(event)" />
    </div>

    
    <div class="row-input col-4-6">
        Đơn vị <i style="color: red">(*)</i>
    </div>
    <div class="row-input col-6-4">
        <input type="text" id="@("_txtUnit")" maxlength="100" onkeydown="PressEnterInsertCategory(event)" />
    </div>


    <div class="row-input col-6-4" style="margin-left: 40%;margin-top:10px">
        <input type="button" id="_btnInsertProduct" value="Chấp nhận" onclick="return SaveCategory()" />
        <input type="button" style="margin-left: 5px" value="Hủy" onclick="return CloseInsertCategory()" />
    </div>
</div>

<script>
    $(document).ready(function () {
        //ChangeAuthorType();

        
    });



</script>
